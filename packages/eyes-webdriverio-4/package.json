{
  "name": "@applitools/eyes.webdriverio",
  "version": "2.34.0",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "repository": {
    "type": "git",
    "url": "https://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "packages/eyes-webdriverio-4"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Applitools Team <team@applitools.com> (http://www.applitools.com/)",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "ttsc",
    "generate:tests": "coverage-tests generate https://raw.githubusercontent.com/applitools/sdk.coverage.tests/universal-sdk/js/config.js --name 'eyes.webdriverio.javascript4'",
    "test": "yarn test:it && yarn test:coverage",
    "test:it": "mocha ./test/it/*.spec.ts --exit --no-timeouts -r ts-node/register -r @applitools/test-utils/mocha-hooks/docker",
    "test:coverage": "yarn generate:tests && APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-webdriverio-4' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "report": "coverage-tests report https://raw.githubusercontent.com/applitools/sdk.coverage.tests/master/js/config.js --name 'eyes.webdriverio.javascript4'",
    "setup": "yarn docker:setup",
    "docker:setup": "node ../scripts/scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "deps": "bongo deps",
    "gh:test": "gh workflow run test.yml --ref $(git rev-parse --abbrev-ref HEAD) -f packages='wdio-legacy' -f links='eyes-sdk-core eyes-api types test-utils utils driver snippets screenshoter sdk-shared visual-grid-client' -f linking-depth=3",
    "gh:publish": "gh workflow run publish.yml --ref $(git rev-parse --abbrev-ref HEAD) -f packages='wdio-legacy'",
    "preversion": "bongo preversion && yarn build",
    "version": "bongo version",
    "postversion": "bongo postversion"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/eyes-api": "1.1.6",
    "@applitools/eyes-sdk-core": "12.24.0",
    "@applitools/types": "1.0.19",
    "@applitools/utils": "1.2.4",
    "@applitools/visual-grid-client": "15.8.44"
  },
  "devDependencies": {
    "@applitools/api-extractor": "1.2.2",
    "@applitools/scripts": "1.0.1",
    "@applitools/sdk-coverage-tests": "2.3.16",
    "@applitools/sdk-release-kit": "^1.0.0",
    "@applitools/sdk-shared": "0.9.8",
    "@applitools/test-utils": "1.0.9",
    "@types/mocha": "^9.0.0",
    "@types/webdriverio": "4.13.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "chromedriver": "^85.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "geckodriver": "^1.19.1",
    "husky": "^4.3.7",
    "mocha": "^8.0.1",
    "prettier": "^2.1.2",
    "spec-xunit-file": "0.0.1-3",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.5.1-rc",
    "webdriverio": "~4.14.4"
  },
  "peerDependencies": {
    "webdriverio": "~4.14.4"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
