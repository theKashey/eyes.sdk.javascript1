{
  "name": "@applitools/driver",
  "version": "1.8.6",
  "description": "Applitools universal framework wrapper",
  "keywords": [
    "applitools",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "packages/driver"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./fake": {
      "default": "./dist/fake/index.js",
      "types": "./types/fake/index.d.ts"
    },
    "./debug": {
      "default": "./dist/debug/index.js",
      "types": "./types/debug/index.d.ts"
    },
    "./dist/*": "./dist/*.js",
    "./dist/fake": "./dist/fake/index.js",
    "./dist/debug": "./dist/debug/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "fake": [
        "./types/fake/index.d.ts"
      ],
      "debug": [
        "./types/debug/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tsc",
    "test": "mocha --no-timeouts -r ts-node/register ./test/**/*.spec.ts",
    "deps": "bongo deps",
    "preversion": "bongo preversion",
    "version": "bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/logger": "1.1.5",
    "@applitools/snippets": "2.2.3",
    "@applitools/types": "1.4.3",
    "@applitools/utils": "1.3.0"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.7",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">= 8.9.0"
  }
}
