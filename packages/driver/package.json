{
  "name": "@applitools/driver",
  "version": "1.5.6",
  "description": "Applitools universal framework wrapper",
  "keywords": [
    "applitools",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "packages/driver"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./fake": {
      "default": "./dist/fake/index.js",
      "types": "./types/fake/index.d.ts"
    },
    "./debug": {
      "default": "./dist/debug/index.js",
      "types": "./types/debug/index.d.ts"
    },
    "./dist/*": "./dist/*.js",
    "./dist/fake": "./dist/fake/index.js",
    "./dist/debug": "./dist/debug/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "fake": [
        "./types/fake/index.d.ts"
      ],
      "debug": [
        "./types/debug/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tsc",
    "test": "mocha --no-timeouts -r ts-node/register ./test/**/*.spec.ts",
    "deps": "bongo deps",
    "preversion": "bongo preversion",
    "version": "bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/snippets": "2.2.2",
    "@applitools/types": "1.2.2",
    "@applitools/utils": "1.2.13"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.7",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 8.9.0"
  }
}
