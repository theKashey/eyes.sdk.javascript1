{
  "name": "@applitools/eyes-universal",
  "version": "0.2.3",
  "description": "Applitools Eyes Universal SDK",
  "keywords": [
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "packages/eyes-universal"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "main": "./index.js",
  "bin": {
    "eyes-universal": "./dist/cli.js"
  },
  "directories": {
    "lib": "./src",
    "example": "./example",
    "test": "./test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "yarn build:dist && yarn build:bin && yarn build:zip && yarn build:tgz",
    "build:dist": "tsc",
    "build:bin": "pkg ./dist/cli.js --out-path ./bin --compress GZip --targets node16-linux-x64,node16-macos-x64,node16-win-x64 && mv ./bin/cli-macos ./bin/eyes-universal-macos && mv ./bin/cli-linux ./bin/eyes-universal-linux && mv ./bin/cli-win.exe ./bin/eyes-universal-win.exe",
    "build:zip": "zip -j ./bin/eyes-universal.zip ./bin/eyes-universal-linux ./bin/eyes-universal-macos ./bin/eyes-universal-win.exe",
    "build:tgz": "tar -czf ./bin/eyes-universal.tar.gz -C ./bin ./eyes-universal-linux ./eyes-universal-macos ./eyes-universal-win.exe",
    "test": "echo 'mocha ./test/**/*.spec.js --no-timeouts'",
    "deps": "bongo deps",
    "preversion": "bongo preversion && yarn build",
    "version": "yarn build && bongo version",
    "postversion": "bongo postversion"
  },
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.24.9",
    "@applitools/logger": "1.0.8",
    "@applitools/utils": "1.2.5",
    "@applitools/visual-grid-client": "15.8.55",
    "webdriver": "7.7.3",
    "ws": "7.4.6",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "^2.3.16",
    "@applitools/sdk-release-kit": "^0.13.4",
    "@applitools/test-utils": "^1.0.10",
    "@applitools/types": "1.0.23",
    "@types/ws": "^7.4.4",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^9.0.0",
    "pkg": "^5.3.2",
    "prettier": "^2.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
